{
  "license": "MIT",
  "name": "@midwest-design/core",
  "public": true,
  "version": "0.0.8",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core/core.js",
  "files": [
    "dist/",
    "loader/",
    "docs/",
    "custom-elements.json"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.ci": "stencil build --ci",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook.start": "start-storybook -p 6006 -s www",
    "storybook.build": "build-storybook -c .storybook -o storybook-dist",
    "generate": "stencil generate",
    "dev": "npm run watch-dev | npm run dev-stencil",
    "dev-stencil": "npm run dev-mode && npm run co-prebuild && stencil build --dev --watch --serve",
    "watch-dev": "node ./build/dev-watch.js"
  },
  "contributes": {
    "html": {
      "customData": [
        "./dist/html.html-data.json"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@f/contains-element": "^1.0.0",
    "@fullcalendar/core": "^5.3.0",
    "@fullcalendar/daygrid": "^5.3.0",
    "@fullcalendar/interaction": "^5.3.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@midwest-design/common": "^0.0.8",
    "@midwest-design/helpers": "^0.0.8",
    "@stencil/core": "latest",
    "@stencil/postcss": "^1.0.1",
    "@stencil/state-tunnel": "^1.0.1",
    "@storybook/addon-a11y": "^6.0.10",
    "@storybook/addon-knobs": "^6.0.10",
    "@storybook/html": "^6.0.10",
    "@types/base-64": "^0.1.3",
    "@types/swiper": "^5.4.0",
    "@types/validator": "^13.1.0",
    "animate-presence": "^0.2.1",
    "async-delay": "^1.0.3",
    "autoprefixer": "^9.8.6",
    "base-64": "^0.1.0",
    "convert-units": "^2.3.4",
    "dayjs": "^1.8.33",
    "debounce-decorator": "^1.0.6",
    "eq.js": "^1.9.0",
    "faker": "^4.1.0",
    "focus-within": "^3.0.2",
    "htmltofigma": "^0.0.6",
    "js-search": "^2.0.0",
    "lit-html": "^1.2.1",
    "my-name-is-url": "^1.4.0",
    "pluralize": "^8.0.0",
    "postcss-design-token-function": "^1.0.0",
    "postcss-import": "^12.0.1",
    "quill": "^1.3.7",
    "quill-emoji": "^0.1.8",
    "quill-mention": "^2.2.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "resize-observer-polyfill": "^1.5.1",
    "round-to": "^4.1.0",
    "source-map-support": "^0.5.19",
    "tailwindcss": "^1.6.2",
    "tailwindcss-animations": "^2.0.0",
    "ts-node": "^8.10.2",
    "twitter-text": "^3.1.0",
    "validator": "^13.1.1",
    "webpack": "^4.44.1",
    "zxcvbn": "^4.4.2"
  }
}
